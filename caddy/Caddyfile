https://{$PRODUCTION_FQDN} {
  import Caddyfile.default
  root production

  git {
    repo {$PRODUCTION_REPO}
    branch {$PRODUCTION_BRANCH}
    hook /_deployhook/production {$WEBHOOK_SECRET}
  }

  proxy /curriculum-vitae https://evansosenko.github.io
  proxy /deck-tmdsc https://evansosenko.github.io
  proxy /deck-ucr-exam https://evansosenko.github.io
  proxy /deck-dichalcogenides-superconductivity https://evansosenko.github.io
  proxy /deck-spin-lifetime https://evansosenko.github.io
}

https://{$STAGING_FQDN} {
  import Caddyfile.default
  root staging
  git {
    repo {$STAGING_REPO}
    branch {$STAGING_BRANCH}
    hook /_deployhook/staging {$WEBHOOK_SECRET}
  }
}
